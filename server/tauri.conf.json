{
  "productName": "wargod",
  "version": "0.0.0",
  "identifier": "wargod.itmeng.top",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "The God of War",
        "width": 1134,
        "height": 860
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "windows": {
      "wix": {
        "language": "zh-CN"
      }
    },
    "active": true,
    "targets": "all",
    "icon": [
      "icons/challenger.png",
      "icons/challenger.ico"
    ]
  }
}
